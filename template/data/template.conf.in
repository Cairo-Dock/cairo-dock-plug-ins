#[gtk-preferences]
[Configuration]

#b This is a boolean parameter. Y/N ?
#{This is a tooltip for this parameter.}
yes_no = false

#i[0;10] This integer can go from 0 to 10 :
int = 0

#s Enter some text here :
text = pouic pouic

